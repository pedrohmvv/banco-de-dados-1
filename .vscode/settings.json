{
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.venv": true,
      "**/.DS_Store": true,
      "**/*.pyc": {
        "when": "$(basename).py"
      },
      "**/__pycache__": true,
      "**/.idea": true,
      "**/node_modules": true
    }
}

